<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tailPath">
	<insert id="insertTailPath" parameterType="String">
			INSERT INTO dogmap(placeNum, placeName)
			VALUES (dogmap_seq.NEXTVAL, #{item})
	</insert>

</mapper>

